      "begin": "[ ._](html|svg)(?:(?:\\s)+)?(?:(?=\\$)|(?:(?:\\n)|$))",
      "end": "\"{3,}",
      "beginCaptures": {
        "0": {
          "name": "punctuation.section.attribute.begin.injection"
        },
        "1": {
          "name": "variable.fsharp.injection"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.section.attribute.end.injection"
        }
      },
      "patterns": [
        {
          "include": "#html_template"
        },
        {
          "include": "source.fsharp"
        }
      ]
    },
    "html_template": {
      "begin": "\\$\"\"\"",
      "end": "(?=\"\"\")",
      "beginCaptures": {
      },
      "endCaptures": {
      },
      "contentName": "template.fsharp.html"
    },
    "sql_injection": {
      "begin": "[ ._](sql)(?:(?:\\s)+)?(?:(?=\\$)|(?:(?:\\n)|$))",
